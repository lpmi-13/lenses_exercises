    "build-prod": "node ./node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=production node ./node_modules/cross-env/dist/bin/cross-env.js BABEL_ENV=production node ./node_modules/webpack/bin/webpack --config webpack.config.js",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "postinstall": "npm run build-prod",
    "test": "jest --collect-coverage",
    "test:update": "jest --updateSnapshot",
    "test:e2e": "start-server-and-test start http://localhost:8000 cy:open",
    "test:headless": "start-server-and-test start http://localhost:8000 cy:run"
